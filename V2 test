loadstring(game:HttpGet("https://raw.githubusercontent.com/Sddnin0/Sddnin/refs/heads/main/V2"))()

-- LocalScript (Đặt trong StarterPlayerScripts hoặc ScreenGui)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- Tạo ScreenGui để chứa menu
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "CuteMenuGui"
ScreenGui.Parent = PlayerGui

-- Tạo Frame làm nền menu chính
local MenuFrame = Instance.new("Frame")
MenuFrame.Name = "MenuBackground"
MenuFrame.Size = UDim2.new(0.35, 0, 0.6, 0) -- Kích thước menu lớn hơn để chứa tabs
MenuFrame.Position = UDim2.new(0.325, 0, 0.2, 0) -- Vị trí menu (ở giữa màn hình)
MenuFrame.BackgroundColor3 = Color3.fromRGB(255, 220, 230) -- Màu hồng pastel
MenuFrame.BorderColor3 = Color3.fromRGB(255, 180, 200) -- Màu viền đậm hơn
MenuFrame.BorderSizePixel = 3
MenuFrame.Parent = ScreenGui

-- Thêm Corner cho Frame để làm tròn góc
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 15) -- Độ cong của góc
UICorner.Parent = MenuFrame

-- Tạo TextLabel làm tiêu đề menu
local TitleLabel = Instance.new("TextLabel")
TitleLabel.Name = "MenuTitle"
TitleLabel.Size = UDim2.new(0.9, 0, 0.1, 0) -- Kích thước tiêu đề nhỏ hơn
TitleLabel.Position = UDim2.new(0.05, 0, 0.03, 0)
TitleLabel.BackgroundColor3 = Color3.new(1, 1, 1) -- Màu trắng
TitleLabel.BackgroundTransparency = 1
TitleLabel.Text = "Sddnin Cute"
TitleLabel.TextColor3 = Color3.fromRGB(100, 50, 70) -- Màu chữ tím than
TitleLabel.Font = Enum.Font.FredokaOne -- Phông chữ dễ thương
TitleLabel.TextSize = 28
TitleLabel.TextScaled = true
TitleLabel.Parent = MenuFrame

-- Khung chứa các nút tab (Script, Misc) và các nút chức năng khác (Discord, Thoát)
local TabButtonsContainer = Instance.new("Frame")
TabButtonsContainer.Name = "TabButtonsContainer"
TabButtonsContainer.Size = UDim2.new(0.9, 0, 0.1, 0) -- Kích thước khung nút
TabButtonsContainer.Position = UDim2.new(0.05, 0, 0.15, 0) -- Đặt dưới tiêu đề
TabButtonsContainer.BackgroundTransparency = 1
TabButtonsContainer.Parent = MenuFrame

-- Layout cho các nút tab (sắp xếp ngang)
local TabListLayout = Instance.new("UIListLayout")
TabListLayout.FillDirection = Enum.FillDirection.Horizontal
TabListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
TabListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
TabListLayout.Padding = UDim.new(0, 5) -- Khoảng cách giữa các nút tab
TabListLayout.Parent = TabButtonsContainer

-- Khung chứa nội dung của các tab
local TabContentContainer = Instance.new("Frame")
TabContentContainer.Name = "TabContentContainer"
TabContentContainer.Size = UDim2.new(0.9, 0, 0.65, 0) -- Kích thước khung nội dung
TabContentContainer.Position = UDim2.new(0.05, 0, 0.28, 0) -- Đặt dưới khung nút
TabContentContainer.BackgroundColor3 = Color3.fromRGB(240, 250, 255) -- Màu nền nhẹ cho nội dung
TabContentContainer.BorderColor3 = Color3.fromRGB(200, 230, 240)
TabContentContainer.BorderSizePixel = 2
TabContentContainer.Parent = MenuFrame

local TabContentCorner = Instance.new("UICorner")
TabContentCorner.CornerRadius = UDim.new(0, 10)
TabContentCorner.Parent = TabContentContainer

-- Hàm tạo nút dễ thương (dùng cho cả tab và nút chức năng)
local function createCuteButton(parent, text, isTabButton)
    local Button = Instance.new("TextButton")
    Button.Size = UDim2.new(isTabButton and 0.24 or 0.8, 0, 1, 0) -- Nút tab nhỏ hơn
    Button.BackgroundColor3 = Color3.fromRGB(200, 255, 220) -- Màu xanh mint pastel
    Button.BorderColor3 = Color3.fromRGB(150, 200, 170)
    Button.BorderSizePixel = 2
    Button.Text = text
    Button.TextColor3 = Color3.fromRGB(50, 100, 70)
    Button.Font = Enum.Font.FredokaOne
    Button.TextSize = 18
    Button.TextScaled = true
    Button.Parent = parent

    local ButtonUICorner = Instance.new("UICorner")
    ButtonUICorner.CornerRadius = UDim.new(0, 8)
    ButtonUICorner.Parent = Button

    -- Hiệu ứng khi di chuột qua nút
    Button.MouseEnter:Connect(function()
        Button.BackgroundColor3 = Color3.fromRGB(220, 255, 240) -- Màu sáng hơn khi hover
    end)

    Button.MouseLeave:Connect(function()
        Button.BackgroundColor3 = Color3.fromRGB(200, 255, 220) -- Trở lại màu cũ
    end)

    return Button
end

-- Tạo các nút tab
local ScriptButton = createCuteButton(TabButtonsContainer, "Script", true)
local MiscButton = createCuteButton(TabButtonsContainer, "Misc", true)
local DiscordButton = createCuteButton(TabButtonsContainer, "Discord", true) -- Giữ Discord là nút tab cho mục đích sắp xếp
local ExitButton = createCuteButton(TabButtonsContainer, "Thoát", true) -- Giữ Thoát là nút tab cho mục đích sắp xếp

-- Khung nội dung cho tab Script
local ScriptContentFrame = Instance.new("Frame")
ScriptContentFrame.Name = "ScriptContent"
ScriptContentFrame.Size = UDim2.new(1, 0, 1, 0)
ScriptContentFrame.Position = UDim2.new(0, 0, 0, 0)
ScriptContentFrame.BackgroundTransparency = 1
ScriptContentFrame.Parent = TabContentContainer

local ScriptContentLayout = Instance.new("UIListLayout")
ScriptContentLayout.Padding = UDim.new(0, 10)
ScriptContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
ScriptContentLayout.VerticalAlignment = Enum.VerticalAlignment.Top
ScriptContentLayout.Parent = ScriptContentFrame

local ScriptLabel = Instance.new("TextLabel")
ScriptLabel.Size = UDim2.new(0.9, 0, 0.2, 0)
ScriptLabel.BackgroundTransparency = 1
ScriptLabel.Text = "Nội dung Script sẽ ở đây!"
ScriptLabel.TextColor3 = Color3.fromRGB(70, 70, 70)
ScriptLabel.Font = Enum.Font.FredokaOne
ScriptLabel.TextSize = 20
ScriptLabel.TextScaled = true
ScriptLabel.Parent = ScriptContentFrame

-- Khung nội dung cho tab Misc
local MiscContentFrame = Instance.new("Frame")
MiscContentFrame.Name = "MiscContent"
MiscContentFrame.Size = UDim2.new(1, 0, 1, 0)
MiscContentFrame.Position = UDim2.new(0, 0, 0, 0)
MiscContentFrame.BackgroundTransparency = 1
MiscContentFrame.Parent = TabContentContainer
MiscContentFrame.Visible = false -- Ban đầu ẩn

local MiscContentLayout = Instance.new("UIListLayout")
MiscContentLayout.Padding = UDim.new(0, 10)
MiscContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
MiscContentLayout.VerticalAlignment = Enum.VerticalAlignment.Top
MiscContentLayout.Parent = MiscContentFrame

local MiscLabel = Instance.new("TextLabel")
MiscLabel.Size = UDim2.new(0.9, 0, 0.2, 0)
MiscLabel.BackgroundTransparency = 1
MiscLabel.Text = "Nội dung Misc sẽ ở đây!"
MiscLabel.TextColor3 = Color3.fromRGB(70, 70, 70)
MiscLabel.Font = Enum.Font.FredokaOne
MiscLabel.TextSize = 20
MiscLabel.TextScaled = true
MiscLabel.Parent = MiscContentFrame

-- Biến theo dõi tab hiện đang hoạt động
local currentActiveTab = ScriptContentFrame

-- Hàm chuyển đổi tab
local function switchTab(newTabContentFrame)
    if currentActiveTab then
        currentActiveTab.Visible = false
    end
    newTabContentFrame.Visible = true
    currentActiveTab = newTabContentFrame
end

-- Kết nối sự kiện nhấp cho các nút tab
ScriptButton.Activated:Connect(function()
    print("Tab Script đã được nhấp!")
    switchTab(ScriptContentFrame)
end)

MiscButton.Activated:Connect(function()
    print("Tab Misc đã được nhấp!")
    switchTab(MiscContentFrame)
end)

DiscordButton.Activated:Connect(function()
    print("Nút Discord đã được nhấp!")
    -- Thêm logic để mở liên kết Discord hoặc hiển thị thông tin Discord
    -- Ví dụ: game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Discord", Text = "Tham gia Discord của chúng tôi: discord.gg/yourinvite"})
end)

ExitButton.Activated:Connect(function()
    print("Nút Thoát đã được nhấp!")
    -- Thêm logic thoát game hoặc đóng menu của bạn ở đây
    ScreenGui:Destroy() -- Đóng menu
end)
